RV_BENCH_BIN=$(dirname $0)/benchmarks/bin
RV_TEST_BIN=$(dirname $0)/tests/bin
RV_INST_BIN=$(dirname $0)/lib/rv-predict-inst.jar
RV_ENGINE_BIN=$(dirname $0)/lib/rv-predict-engine.jar
RV_LOG_BIN=$(dirname $0)/lib/rv-predict-log.jar

RV_RECORD_BIN=$(dirname $0)/tmp/record

#record
echo "logging ""$@"
time java -Xmx32g -cp $RV_RECORD_BIN:$RV_BENCH_BIN:$RV_TEST_BIN:$RV_LOG_BIN:$RV_ENGINE_BIN edu.uiuc.run.Main "$@"
